{
    "manifest_version": 3,
    "name": "Disable Images for Carbon Reduction",
    "version": "1.0",
    "description": "Desativa imagens para reduzir consumo de dados e emiss√µes de carbono.",
    "icons": {
      "128": "icon.png"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  
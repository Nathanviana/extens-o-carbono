{
    "manifest_version": 3,
    "name": "Minha Extensão",
    "version": "1.0",
    "description": "Descrição da minha extensão",
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  